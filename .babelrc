{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-react"
    ],
    "plugins": [
        [
            "@babel/plugin-proposal-class-properties",
            {
                "loose": true
            }
        ],
        [
            "transform-remove-console",
            {
                "exclude": ["error", "warn"]
            }
        ]
    ]
}